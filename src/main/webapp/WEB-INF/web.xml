<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  id="WebApp_ID" version="3.1">

  <display-name>Metamind API</display-name>
  <description>Metamind API</description>
  <module-name>metamind</module-name>
  <distributable/>
  
   <security-constraint>

    <web-resource-collection>

      <web-resource-name>files</web-resource-name>

      <url-pattern>/v2/images</url-pattern>

    </web-resource-collection>

  </security-constraint>



  <security-constraint>

    <web-resource-collection>

      <web-resource-name>fileslist</web-resource-name>

      <url-pattern>/v2/images/*</url-pattern>

    </web-resource-collection>

  </security-constraint>
  
</web-app>
