embed-server --server-config=standalone-full-ha.xml
/subsystem=datasources/jdbc-driver=mariadb-xa:add(driver-name=mariadb-xa, driver-module-name=com.mariadb.jdbc, xa-datasource-class=org.mariadb.jdbc.MySQLDataSource)
data-source add --name=metamind --jndi-name=java:jboss/datasources/metamind --enabled=true --jta=true --use-ccm=true --connection-url=jdbc:mysql://${env.METAMIND_API_DB_HOST}:${env.METAMIND_API_DB_PORT}/${env.METAMIND_API_DB_NAME}?useUnicode=true&characterEncoding=UTF-8 --driver-name=mariadb-xa --user-name=${env.METAMIND_API_DB_USERNAME} --password=${env.METAMIND_API_DB_PASSWORD} --check-valid-connection-sql="SELECT 1" --validate-on-match=false --share-prepared-statements=false --max-pool-size=1000